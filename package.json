{
  "name": "ng2-rest-docs-server",
  "version": "1.0.20",
  "description": "Generate docs for angular 2 app with ng2-rest lib in easy way",
  "main": "karma.conf.js",
  "directories": {
    "doc": "docs"
  },
  "bin": {
    "ng2-rest-docs-server": "bin/ng2-rest-docs-server"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "lodash": "^4.17.2",
    "md5": "^2.2.1",
    "method-override": "^2.3.6"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.1.2",
    "ts-node": "^1.3.0",
    "typescript": "^2.0.10"
  },
  "scripts": {
    "test": "mocha --require ts-node/register src/**/*.spec.ts",
    "test:watch": "mocha --compilers ts-node/register src/**/*.spec.ts --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darekf77/ng2-rest-docs-server.git"
  },
  "keywords": [
    "ng2-rest",
    "docs",
    "documentation"
  ],
  "author": "Dariusz Filipiak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/darekf77/ng2-rest-docs-server/issues"
  },
  "homepage": "https://github.com/darekf77/ng2-rest-docs-server#readme"
}
